{
  "name": "selectors",
  "version": "0.5.0",
  "description": "A set of building blocks for creating complex selector functions",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "babel src --out-dir lib --ignore '*.test.js'",
    "build-watch": "watch 'npm run build' ./src",
    "clean": "rimraf lib coverage",
    "coverage": "jest --coverage src",
    "lint": "eslint src",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theclinician/selectors.git"
  },
  "jest": {
    "testURL": "http://localhost/",
    "testPathIgnorePatterns": [
      "/node_modules",
      "/lib"
    ]
  },  
  "keywords": [
    "selector",
    "redux",
    "reselect"
  ],
  "author": "apendua",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theclinician/selectors/issues"
  },
  "homepage": "https://github.com/theclinician/selectors#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-root-import": "^4.1.8",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^22.4.3",
    "npm-watch": "^0.3.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "reselect": "^4.0.0"
  }
}
